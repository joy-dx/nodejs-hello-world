{
  "openapi": "3.0.3",
  "info": {
    "title": "GOHW API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:{port}",
      "variables": {
        "port": {
          "default": "3000"
        }
      }
    }
  ],
  "paths": {
    "/": {
      "get": {
        "operationId": "getRoot",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "message": { "type": "string" }
                  },
                  "required": ["message"]
                },
                "examples": {
                  "ok": {
                    "value": { "message": "Hello from JoyDX" }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/health": {
      "get": {
        "operationId": "getHealth",
        "responses": {
          "200": {
            "description": "Service health check",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "status": { "type": "string" }
                  },
                  "required": ["status"]
                },
                "examples": {
                  "ok": { "value": { "status": "ok" } }
                }
              }
            }
          }
        }
      }
    },
    "/test-credential-leak": {
      "get": {
        "operationId": "getTestCredentialLeak",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "secret": { "type": "string" }
                  },
                  "required": ["secret"]
                },
                "examples": {
                  "example": {
                    "value": {
                      "secret": "BIzaSyDaGmWKa4JsXZ-HjGw7ISLn_3namBGfuQe"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}